	<!-- Google Tag Manager Datalayer - Part 1 of 4 - START -->
    <script type="text/javascript">
        window.dataLayer = window.dataLayer || [];
        function getCookieValue(name) {
            var value = document.cookie.match('(^|;)\\s*' + name + '\\s*=\\s*([^;]+)');
            return value ? xssFilter(value.pop()) : '';
        }
        function getCustomerDetailsCookieValue() {
            var encodedCustomerDetails = getCookieValue('customerDetails');
            var decodedCustomerDetails = atob(encodedCustomerDetails);
            return xssFilter(decodedCustomerDetails).split(',');
        }
        function xssFilter(unsanitzedString) {
            var lt = /</g,
                gt = />/g,
                ap = /'/g,
                ic = /"/g;
            return unsanitzedString.toString().replace(lt, "&lt;").replace(gt, "&gt;").replace(ap, "&#39;").replace(ic, "");
		}
        var customerType;
        var customerTypeCookie = getCookieValue('customerType');
        customerType = customerTypeCookie ? (customerTypeCookie === 'newCustomer' ? 'New' : 'Existing') : 'Guest';

        var customerDetails = getCustomerDetailsCookieValue();
        function isUserLoggedIn() {
            return getCustomerDetailsCookieValue()[0] === 'true';
        }

        var userStatus = isUserLoggedIn() ? 'logged-in' : 'logged-out';
        var userID = customerDetails[1] ? customerDetails[1] : undefined;
        var isLybcMember = customerDetails[5] === 'true' ? "Y" : "N";
        var customerBucket = customerDetails[6]  ? customerDetails[6] : 'Undefined';
        var shippedOrders = customerDetails[7] ? customerDetails[7] : 0;
        
				dataLayer.push({
					'event': 'pageDataLoaded',
					'userID': userID,
					'customerType' : customerType,
					'userStatus': userStatus,
					'lybcMembership': isLybcMember,
                    'userBucket': customerBucket,
                    'completedOrders': shippedOrders
				});
			</script>
    <!-- Google Tag Manager Datalayer - Part 1 of 4 - END -->

    <!-- Google Tag Manager Data Attributes - Part 2 of 4 - START -->
<script type="text/javascript">

    
	function createMetaTag(name, content)
	{
		var dtmMetaTag = document.createElement('meta');
		dtmMetaTag.setAttribute('id', name);
		dtmMetaTag.setAttribute('name', name);
		dtmMetaTag.setAttribute('content', content);
		document.getElementsByTagName('head')[0].appendChild(dtmMetaTag);
	}

	var customerEmail = customerDetails[4];
	if (customerEmail)
    {
        createMetaTag('dtmCustomerEmail', customerEmail);
        createMetaTag('dtmCustomerFN', customerDetails[2]);
        createMetaTag('dtmCustomerLN', customerDetails[3]);
    }
	
	createMetaTag('dtmDeviceType', 'Desktop');
	createMetaTag('dtmDomain', 'en-ca');
	createMetaTag('dtmCurrency', 'CAD');
	
    createMetaTag('customerType', customerType);

	
			var dtmCategoryList = [];
			
					dtmCategoryList.push('Gifts');
				
					dtmCategoryList.push('Gifts Sale');
				
			createMetaTag('dtmCategoryList', dtmCategoryList.join('|'));
		</script>
<!-- Google Tag Manager Data Attributes - Part 2 of 4 - END -->
